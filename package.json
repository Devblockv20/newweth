{
  "name": "weifund-dapp-basic",
  "version": "0.1.0",
  "description": "A basic limited JS framework high preformance WeiFund client",
  "main": "index.js",
  "scripts": {
    "start": "node ./node_modules/budo/bin/cmd.js src/app.js:bundle.js -P --cors --open -- -t [ babelify --presets [ es2015 ] ]",
    "browserify": "browserify src/app.js -o bundle.js -t [ babelify --presets [ es2015 ] ]",
    "surge": "npm run browserify && cp index.html 200.html && surge --domain weifund-basic.surge.sh && rm 200.html && rm bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/weifund/weifund-dapp-basic.git"
  },
  "keywords": [
    "dapp",
    "ethereum",
    "weifund"
  ],
  "author": "Nick Dodson <thenickdodson@gmail.com>, Niran Babalola <niran.babalola@consensys.net>",
  "license": "TBD",
  "bugs": {
    "url": "https://github.com/weifund/weifund-dapp-basic/issues"
  },
  "homepage": "https://github.com/weifund/weifund-dapp-basic#readme",
  "dependencies": {
    "eth-lightwallet": "2.5.3",
    "ipfs-js": "0.0.11",
    "node-polyglot": "2.0.0",
    "qrious": "2.0.2",
    "sheet-router": "4.0.0-0",
    "store": "1.3.20",
    "xss": "0.2.13",
    "yo-yo": "1.3.2",
    "web3": "0.15.3",
    "web3-network-detective": "0.1.1",
    "weifund-contracts": "2.0.3",
    "weifund-lib": "0.1.3",
    "weifund-util": "git+https://github.com/weifund/weifund-util"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.7.7",
    "babel-eslint": "6.0.4",
    "babelify": "7.3.0",
    "budo": "9.2.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0"
  }
}
